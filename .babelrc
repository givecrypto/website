{
  "presets": ["@babel/preset-env"],
  "env": {
    "development": {
      "presets": ["next/babel", "@zeit/next-typescript/babel"],
      "plugins": ["inline-dotenv", "inline-react-svg", "preval"]
    },
    "production": {
      "presets": ["next/babel", "@zeit/next-typescript/babel"],
      "plugins": ["transform-inline-environment-variables", "inline-react-svg", "preval"]
    },
    "test": {
      "presets": ["es2016", "next/babel", "@zeit/next-typescript/babel"],
      "plugins": ["inline-dotenv", "inline-react-svg", "preval"]
    }
  }
}
